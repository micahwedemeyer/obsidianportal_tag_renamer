<!DOCTYPE html>
<html>
<head>
  <title>Obsidian Portal Tag Renamer</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="header">
</div> <!-- header -->

<div id="container">
<%= yield %>
</div> <!-- container -->

<div id="footer">
  <% if logged_in? %>
    <%= link_to 'Logout', logout_path %>
  <% end %>
</div> <!-- footer -->

</body>
</html>
